    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "tailwindcss": "^3.4.14",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "esbuild": "^0.27.0"
  }
}
